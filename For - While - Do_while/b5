#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    int i,n;
    printf("Nhap so gi di: ");
    scanf("%d",&n);
    printf("%d co cac uoc so : {",n);
    for(i=1; i<=n; i++)
    {
        if(n%i==0)
        {
            printf("%d, ",i);
        }
    }
    printf("\b\b}");
    return 0;
}
